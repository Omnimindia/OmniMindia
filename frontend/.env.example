# OmniMindia Frontend Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME=OmniMindia
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI Solutions Powered by CV, GenAI, ML, LLM, Robotics & Machine Vision

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_OMNI_WIDGET=true
VITE_ENABLE_THREE_NUCLEUS=true

# Analytics (Optional)
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# Social Media Links
VITE_GITHUB_URL=https://github.com/omnimindia
VITE_TWITTER_URL=https://twitter.com/omnimindia
VITE_LINKEDIN_URL=https://linkedin.com/company/omnimindia

# Contact
VITE_CONTACT_EMAIL=contact@omnimindia.com
VITE_SUPPORT_EMAIL=support@omnimindia.com

# Production URLs
# VITE_API_URL=https://api.omnimindia.com
# VITE_WEBSITE_URL=https://www.omnimindia.com
